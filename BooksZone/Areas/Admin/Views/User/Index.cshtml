
@{
    ViewData["Title"] = "Index";
}

<div class="row mt-5">
    <div class="col-6">
        <h2 class="text-primary">User List</h2>
    </div>
    <div class="col-6 text-right">
        <a class="btn btn-primary text-white" asp-area="Identity" asp-page="/Account/Register"><i class="fa fa-plus"></i> &nbsp; Create New User</a>
    </div>
</div>

<div class="mt-4 p-4 border rounded">
    <table id="tableData" class="table table-striped table-bordered">
        <thead class="table-dark">
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Company</th>
                <th>Role</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/user.js"></script>
}

